
<div class="row">
  <div class="col-sm-3"></div>
    <div class="col-sm-6" class="list-group" id="burgers">
      <div class="list-group-item list-group-item-action">
        <h5>Burgers/Sandwiches</h5>
          {{#each itm}}
            {{#if this.entree}}
              <p>
                {{this.id}}. {{this.item}}
              </p>
            {{/if}}
          {{/each}}
      </ul>
    </div>
  </div>
</div>

<div class="row">
  <div class="col-sm-3"></div>
  <div class="col-sm-6" class="list-group" id ="sides">
    <ul class="list-group-item list-group-item-action">
      <h5>Sides</h5>
        {{#each itm}}
          {{#unless entree}}
              <p>
                {{this.id}}. {{this.item}}
              </p>
          {{/unless}}
        {{/each}}
    </ul>
  </div>
</div>
